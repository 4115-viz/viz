func main(): none {

    @@x: int;
    @@y: float;
    @@z: bool;
    @@a: bool;
    @@b: string;

    @x = 1;
    @y = 1.0;
    @z = true;
    @a = false;
    @b = "Hello World";  
    
    print("Assigning literals to variables");
    print("Accessing the variables and printing values");
    print_int(@x);
    print_float(@y);
    print_bool(@z);
    print_bool(@a);
    print(@b);

}